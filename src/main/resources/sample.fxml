<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" minWidth="850.0" prefWidth="850.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.ether.forms.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <children>
        <BorderPane>
            <center>
                <TableView fx:id="table" editable="true" prefWidth="850.0" BorderPane.alignment="CENTER">
                    <columns>
                        <TableColumn fx:id="idColumn" prefWidth="26.0" text="Id" />
                        <TableColumn fx:id="nameColumn" minWidth="150.0" prefWidth="220.0" text="Name" />
                  <TableColumn fx:id="urlColumn" minWidth="150.0" prefWidth="220.0" text="URL" />
                  <TableColumn fx:id="createDateColumn" minWidth="100.0" prefWidth="150.0" text="Create Date" />
                  <TableColumn fx:id="modificationDateColumn" minWidth="100.0" prefWidth="150.0" text="Modification Date" />
                  <TableColumn fx:id="intervalColumn" minWidth="50.0" prefWidth="70.0" text="Interval" />
                    </columns>
                </TableView>
            </center>
            <bottom>
                <HBox prefWidth="219.0" BorderPane.alignment="CENTER">
                    <children>
                        <TextField fx:id="name" promptText="Name" />
                        <TextField fx:id="url" promptText="URL" />
                        <Button mnemonicParsing="false" onAction="#addWebSite" text="+" />
                    </children>
                </HBox>
            </bottom>
        </BorderPane>
    </children>
</GridPane>
